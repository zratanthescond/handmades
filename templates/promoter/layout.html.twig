<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}{% endblock %} - Paramall</title>
        <meta name="robots" content="noindex, nofollow, noarchive, nosnippet, noodp, noimageindex, notranslate, nocache" />
        <meta name="viewport" content="width=device-width,initial-scale=1"/>

        {% block stylesheets %}
           {{ encore_entry_link_tags('promoter') }}
        {% endblock %}

        {% block javascripts %}
            {{ encore_entry_script_tags('promoter') }}
        {% endblock %}
    </head>

<body>

{% block body %}

{% include "promoter/partial/header.html.twig" %}

<div id="main" class="container mt-5 mb-5">

<div class="row">

<div class="col-md-12">

 {% for label, messages in app.flashes(['danger', 'success']) %}
    
    {% for message in messages %}
     
    <div class="temporary-alert mb-4 alert alert-{{ label }} text-center font-weight-bold"> 
    
    <i class="fa fa-info-circle" aria-hidden="true"></i>
    
    <span class="ml-2">{{ message }} </span>
    
    </div>

    {% endfor %}
{% endfor %}

{% block main %} {% endblock %}

</div>

</div>

</div>

{% endblock %}

    </body>
</html>
